{
  "name": "cypress-grep-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:filter": "npm run cy:run -- --env GREP_PRE_FILTER=true",
    "cy:grep": "export CYPRESS_GREP_RESULTS_FILE='./filtered_tests.json' && npm run cy:filter && npm run cy:run -- --config specPattern='*.*no'",
    "cy:run:filtered": "CYPRESS_GREP_RESULTS_FILE='./filtered_tests.json' npm run cy:run -- --config specPattern='*.*no'",
    "cy:p1": "CYPRESS_GREP='@P1' npm run cy:grep",
    "cy:p1:filtered": "CYPRESS_GREP='@P1' npm run cy:run:filtered"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmisty/cypress-grep-example.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmisty/cypress-grep-example/issues"
  },
  "homepage": "https://github.com/mmisty/cypress-grep-example#readme",
  "dependencies": {
    "@mmisty/cypress-grep": "^1.4.6",
    "cypress": "^12.17.4"
  }
}
